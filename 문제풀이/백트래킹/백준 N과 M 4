
def dfs(start):
    if len(result) == M:
        print(' '.join(map(str,result)))
        return
    
    for i in range(start, N+1):
        result.append(i)
        dfs(i)
        result.pop()


N,M = map(int, input().split())
result = []

dfs(1)


#입력
#4 2

#출력
# 1 1
# 1 2
# 1 3
# 1 4
# 2 2
# 2 3
# 2 4
# 3 3
# 3 4
# 4 4
